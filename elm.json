{
    "type": "package",
    "name": "jfmengels/elm-review-the-elm-architecture",
    "summary": "Provides elm-review rules to improve your use of The Elm Architecture",
    "license": "BSD-3-Clause",
    "version": "1.0.2",
    "exposed-modules": [
        "NoMissingSubscriptionsCall",
        "NoRecursiveUpdate",
        "NoUselessSubscriptions"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.2 <= v < 2.0.0",
        "jfmengels/elm-review": "2.3.2 <= v < 3.0.0",
        "stil4m/elm-syntax": "7.1.3 <= v < 8.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.2.2 <= v < 2.0.0"
    }
}
